resource students
	name string
	classrooms classrooms array

resource classrooms
	number string

if size( students ) == 0
	batchInsert( students, [["rayna"], ["carys"], ["soren"], ["larkin"]] )
	batchInsert( classrooms, [["101"], ["105"], ["208"], ["302"], ["307"]] )
	associate( students, "name", "rayna", classrooms, "number", "105" )

show( "students$$classrooms" )
print( findOne( students, "name", "soren" ) )